{
  "name": "@zigy/dat-menubar",
  "version": "0.0.1",
  "description": "Electron based menubar app for dat-socket",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "electron": "electron .",
    "build:electron": "electron-packager . dat-menubar  --platform=linux --arch=x64 --electron-version=2.0.5 --overwrite=true",
    "build:browserify": "browserify app.js -o bundles/bundle.js -t yo-yoify -t  [ sheetify -u sheetify-cssnext ] -g es2040 -p [ css-extract -o bundles/bundle.css ] -p tinyify"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@zigy/dat-socket": "^0.0.1",
    "choo": "^6.13.0",
    "choo-devtools": "^2.5.1",
    "dat-colors": "^3.5.1",
    "electron-rpc": "^2.0.1",
    "es2040": "^1.2.6",
    "hyperscript": "^2.0.2",
    "menubar": "^5.2.3",
    "server-destroy": "^1.0.1",
    "tachyons": "^4.11.1"
  },
  "devDependencies": {
    "browserify": "^16.2.2",
    "css-extract": "^1.3.0",
    "electron": "^2.0.5",
    "electron-packager": "^12.1.0",
    "sheetify": "^7.3.3",
    "tinyify": "^2.4.3",
    "yo-yoify": "^4.3.0"
  }
}
