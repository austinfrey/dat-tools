{
  "name": "@zigy/dat-socket",
  "version": "0.0.3",
  "description": "use in a project `npm install --save zigy/dat-socket`",
  "main": "index.js",
  "bin": {
    "dat-socket": "bin.js"
  },
  "scripts": {
    "test": "tape tests/*.js",
    "dat-socket": "node bin 3000",
    "pm2": "pm2 start bin.js",
    "lint": "xo --fix"
  },
  "xo": {
    "semicolon": false,
    "rules": {
      "no-undef": [
        "warn",
        {
          "typeof": true
        }
      ],
      "prefer-destructuring": [
        "error",
        {
          "object": true,
          "array": false
        }
      ]
    }
  },
  "keywords": [
    "dat",
    "hyperdrive",
    "hypercore",
    "websockets"
  ],
  "author": "Austin Frey",
  "license": "MIT",
  "dependencies": {
    "dataplex": "^3.0.2",
    "hyperdiscovery": "^8.0.0",
    "hyperdrive": "git+https://github.com/mafintosh/hyperdrive.git#hyperdb-backend",
    "pump": "^3.0.0",
    "random-access-memory": "^3.0.0",
    "routes": "^2.1.0",
    "signalhub": "^4.9.0",
    "stoppable": "^1.0.6",
    "to-buffer": "^1.1.1",
    "websocket-stream": "^5.1.2"
  },
  "devDependencies": {
    "faucet": "^0.0.1",
    "tap-difflet": "^0.7.1",
    "tape": "^4.9.1"
  },
  "gitHead": "b217066f397e15db1141bcf06bafc61356c6ccb3"
}
